{#
/**
 * @file
 * Parc Component.
 */
#}

{{ attach_library('parc/s--footer') }}

{% set footer_attribute = create_attribute() %}

{% set footer_classes = [
  'bg-black',
] %}

<footer {{ footer_attribute.setAttribute('id', 'footer').addclass(footer_classes) }}>
  <div class="container">
    <div class="row row-top align-items-start">
      {{- page.footer -}}
      {{- page.footer_second -}}
    </div>

    {{ page.footer_s }}
  </div>
</footer>
