{#
/**
 * @file
 * Parc theme implementation for main view template.
 */
#}

{{ attach_library('parc/v--deliverables') }}

{%
  set classes = [
    dom_id ? 'js-view-dom-id-' ~ dom_id,
  ]
%}

<div{{attributes.addClass(classes)}}>
  {{ title_prefix }}
  <div class="svg-title">
    <h1>
      {{ title }}
    </h1>
  </div>
  {{ title_suffix }}

  {% if header %}
    <header>
      {{ header }}
    </header>
  {% endif %}

  {{ exposed }}

  {% if rows -%}
    {{ rows }}
  {% elseif empty -%}
    {{ empty }}
  {% endif %}
  {{ pager }}
</div>
