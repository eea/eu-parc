{#
/**
 * @file
 * Parc theme implementation for a field.
 */
#}

<div class="grid-sm">
  {%- for item in items -%}
    <div class="position-relative">
      <div class="paragraph--count">
        #{{ items|length - loop.index0 -}}
      </div>

      {{- item.content -}}
    </div>
  {%- endfor -%}
</div>
