{{ attach_library('edwt/accordion') }}
{{ attach_library('edwt/tooltip') }}
{{ attach_library('edwt/popover') }}
{{ attach_library('parc/parc-popper') }}

<div class="interactive-map" data-map-id="{{ map_id }}" id="interactive-map-{{ map_id }}" style="position: relative; height: 1400px;">
  <div class="results">
    <div class="accordion" id="accordionInfo">
      <div class="accordion-item">
        <h2 class="accordion-header" id="headingOne">
          <button id="showResultsButton" class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#collapseOne" aria-expanded="false" aria-controls="collapseOne">
            &nbsp;
          </button>
        </h2>
        <div id="collapseOne" class="accordion-collapse collapse" aria-labelledby="headingOne" data-bs-parent="#accordionExample">
          <div id="resultsParent" class="accordion-body">
          </div>
        </div>
      </div>
    </div>

    <div id="identifyParent" style="display: none;">
      <div id="selectedFeatureParent">
      </div>
    </div>
  </div>

  <div style="display: none;">
    <!-- Popup -->
    <div id="popup"></div>
  </div>
</div>
