<div class="indicator-chart__wrapper">
  <div class="indicators-timeline indicator-chart">
    {% for item in items %}
      <div class="column">
        {% for it in item %}
          <a class="image" href="{{ it.link }}">{{ it.image }}</a>
        {% endfor %}
        <div class="date">
          {{ item.0.date }}
        </div>
      </div>
    {% endfor %}
  </div>
</div>
