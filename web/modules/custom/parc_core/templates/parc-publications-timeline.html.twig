<div class="container">
  {% for item in items %}
    <div class="column">
      {% for it in item %}
        <a class="image" href="{{ it.link }}">{{ it.image }}</a>
      {% endfor %}
      <div class="date">
        <h5 style="rotate:-30deg; margin-top:20px;">{{ item.0.date }}</h5>
      </div>
    </div>
  {% endfor %}
</div>

<style>
  .container {
    display: flex;
    align-items: flex-end; /* Align items to the top */
    justify-content: space-between;
    padding: 20px;
  }
  .column {
    display: flex;
    flex-direction: column;
    align-items: center; /* Center align items */
    width:120px;
  }
  .image {
    margin: 5px;
    align-self: flex-end; /* Align image at the bottom of the column */
  }

  .date {
      text-align:end;
      margin: 5px;

  }
</style>
