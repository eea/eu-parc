{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 9 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "~1.0",
        "drupal/address": "^1.11",
        "drupal/addtoany": "^2.0",
        "drupal/admin_toolbar": "^3.3",
        "drupal/allowed_formats": "^2.0",
        "drupal/backup_migrate": "^5.0",
        "drupal/better_exposed_filters": "^6.0",
        "drupal/block_class": "^2.0",
        "drupal/calendar_link": "^2.1",
        "drupal/ckeditor_bs_grid": "^2.0",
        "drupal/ckeditor_media_embed": "^1.11",
        "drupal/ckeditor_templates": "^1.2",
        "drupal/ckeditor_templates_ui": "^1.4",
        "drupal/color_field": "^2.5",
        "drupal/colorbutton": "^1.5",
        "drupal/core-composer-scaffold": "^9.5",
        "drupal/core-project-message": "^9.5",
        "drupal/core-recommended": "^9.5",
        "drupal/ctools": "^4.0",
        "drupal/devel": "^5.1",
        "drupal/edwt": "2.1.4",
        "drupal/extlink": "^1.7",
        "drupal/facets": "^2.0",
        "drupal/field_group": "^3.4",
        "drupal/geocoder": "^3.32",
        "drupal/geofield": "^1.52",
        "drupal/geolocation": "^3.12",
        "drupal/ics_link_field": "^1.0@alpha",
        "drupal/image_field_caption": "^1.2",
        "drupal/layout_builder_admin_theme": "^1.1",
        "drupal/layout_builder_component_attributes": "^2.1",
        "drupal/layout_builder_modal": "^1.2",
        "drupal/layout_builder_styles": "^2.0",
        "drupal/link_class": "^2.0",
        "drupal/menu_block": "^1.10",
        "drupal/menu_link_attributes": "^1.3",
        "drupal/menu_trail_by_path": "^2.0",
        "drupal/metatag": "^1.22",
        "drupal/paragraphs": "^1.15",
        "drupal/pathauto": "^1.11",
        "drupal/rabbit_hole": "^1.0@beta",
        "drupal/search_api": "^1.29",
        "drupal/selective_better_exposed_filters": "^3.0@beta",
        "drupal/simple_sitemap": "^4.1",
        "drupal/smart_date": "^4.0@alpha",
        "drupal/smart_trim": "^2.0",
        "drupal/svg_image": "^1.16",
        "drupal/taxonomy_menu": "^3.6",
        "drupal/twig_tweak": "^3.2",
        "drupal/ultimate_cron": "^2.0@alpha",
        "drupal/views_ajax_history": "^1.7",
        "drupal/views_custom_cache_tag": "^1.3",
        "drupal/views_infinite_scroll": "^2.0",
        "drupal/viewsreference": "^2.0@beta",
        "drush/drush": "^11.4",
        "eaudeweb/heavy-lifter": "^2.0",
        "symfony/css-selector": "^5.4"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "cweagans/composer-patches": true,
            "php-http/discovery": true
        },
        "sort-packages": true,
        "platform-check": false
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            },
            "file-mapping": {
                "[web-root]/sites/development.services.yml": false
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/installing-drupal",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        },
        "enable-patching": true,
        "patches": {
            "drupal/color_field": {
                "3064418 HTML5 widget for unlimited values needs improvements":"https://www.drupal.org/files/issues/2020-12-12/3064418-12--color-field-html5-widget-clear.patch"
            },
            "drupal/facets": {
                "Facets with AJAX not working in most of situations": "https://www.drupal.org/files/issues/2023-02-07/facets-with-ajax-not-working-in-most-of-situations-3052574-174.patch"
            }
        }
    },
    "require-dev": {
        "drupal/stage_file_proxy": "^2.0"
    }
}
